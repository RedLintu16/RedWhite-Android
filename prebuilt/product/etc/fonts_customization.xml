    <!-- Raleway -->
    <family customizationType="new-named-family" name="raleway">
        <font weight="300" style="normal">Raleway-Light.ttf</font>
        <font weight="400" style="normal">Raleway.ttf</font>
        <font weight="600" style="normal">Raleway-SemiBold.ttf</font>
        <font weight="700" style="normal">Raleway-Bold.ttf</font>
        <font weight="500" style="normal">Raleway-Medium.ttf</font>
    </family>
    <alias name="raleway-light" to="raleway" weight="300" />
    <alias name="raleway-regular" to="raleway" weight="400" />
    <alias name="raleway-medium" to="raleway" weight="500" />
    <alias name="raleway-semi-bold" to="raleway" weight="600" />
    <alias name="raleway-bold" to="raleway" weight="700" />